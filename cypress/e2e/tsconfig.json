{
  "compilerOptions": {
    "skipLibCheck": true,

    /* JavaScript Support */
    "allowJs": true,
    "checkJs": true,

    /* Type Checking */
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,

    "paths": {
      "@/*": ["./*"]
    }
  }
}
